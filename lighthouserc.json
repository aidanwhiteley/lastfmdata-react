{
    "ci": {
        "collect": {
            "numberOfRuns": 3
        },
        "assert": {
            "preset": "lighthouse:no-pwa",
            "assertions": {
                "categories:performance": ["warn", {
                    "minScore": 0.9
                }],
                "categories:accessibility": ["error", {
                    "minScore": 0.8
                }]
            }
        },
        "upload": {
            "target": "temporary-public-storage"
        }
    }
}